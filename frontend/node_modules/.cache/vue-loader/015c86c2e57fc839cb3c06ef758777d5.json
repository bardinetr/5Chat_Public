\">\r\n              <b-form-input\r\n                v-model=\"username\"\r\n                id=\"inline-form-input-username\"\r\n                placeholder=\"Username\"\r\n                :state=\"usernameIsValid\"\r\n              ></b-form-input>\r\n            </b-input-group>\r\n          </b-form>\r\n        </div>\r\n        <b-button class=\"button\" variant=\"outline-primary\" @click=\"callFakeUser\"\r\n          >Populate fake user</b-button\r\n        >\r\n        <p></p>\r\n        <b-button\r\n          class=\"button\"\r\n          variant=\"outline-primary\"\r\n          @click=\"callUser\"\r\n          :disabled=\"!usernameIsValid\"\r\n          >Populate user</b-button\r\n        >\r\n        <div :hidden=\"!accountIsPrivate\"> This account is private </div>\r\n        <p></p>\r\n      </div>\r\n    </div>\r\n\r\n    <TwitterData :userData=\"user\" />\r\n    <GeneralData :userData=\"user\" v-on:updateUserData=\"updateUserGeneralData\" />\r\n    <FiltersData :userData=\"user\" v-on:updateUserData=\"updateUserFiltersData\" />\r\n    <ScoreData :userData=\"user\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport userServices from \"@/services/userServices.js\";\r\nimport TwitterData from \"@/components/TwitterData.vue\";\r\nimport GeneralData from \"@/components/GeneralData.vue\";\r\nimport FiltersData from \"@/components/FiltersData.vue\";\r\nimport ScoreData from \"@/components/ScoreData.vue\";\r\nexport default {\r\n  name: \"Testing\",\r\n  components: {\r\n    TwitterData,\r\n    GeneralData,\r\n    FiltersData,\r\n    ScoreData,\r\n  },\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      user: null,\r\n    };\r\n  },\r\n  computed: {\r\n    usernameIsValid: function () {\r\n      if (this.username != null && this.username.length >= 4) {\r\n        return true;\r\n      } else if (this.username != null) {\r\n        return false;\r\n      }\r\n\r\n      return null;\r\n    },\r\n    accountIsPrivate: function () {\r\n      if (this.user != null && this.user.twitter != null) {\r\n        if (this.user.twitter.likes == undefined) {\r\n          return true;\r\n        }\r\n      }\r\n      return false;\r\n    },\r\n  },\r\n  methods: {\r\n    async callFakeUser() {\r\n      userServices.getFakeUser().then((res) => {\r\n        this.user = res;\r\n        this.username = res.twitter.acc;\r\n        console.log(this.user);\r\n      });\r\n    },\r\n    async callUser() {\r\n      if (this.username !== null) {\r\n        userServices.getUser(this.username).then((res) => {\r\n          this.user = res;\r\n          console.log(\"returned user data =\", this.user);\r\n        });\r\n      }\r\n    },\r\n    updateUserGeneralData(userGeneral) {\r\n      this.user.general = userGeneral;\r\n      userServices.postUserGeneral(this.username, userGeneral).then((res) => {\r\n        console.log(res.data);\r\n      });\r\n    },\r\n    updateUserFiltersData(userFilters) {\r\n      this.user.filters = userFilters;\r\n      userServices.postUserFilters(this.username, userFilters).then((res) => {\r\n        console.log(res.data);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#button-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: left;\r\n}\r\n\r\n#testing-compo {\r\n  border: 5px solid rgb(0, 255, 115);\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: left;\r\n  padding: 3rem;\r\n}\r\n\r\n.button {\r\n  width: max-content;\r\n}\r\n</style>"]}]}