t id=\"langlist-id\">\r\n            <option>Manual Option</option>\r\n            <option v-for=\"lang in langList\" :key=\"lang.name\">\r\n              {{ lang.name }}\r\n              <!-- <span>  ( {{lang.native}}  ) </span> -->\r\n            </option>\r\n          </datalist>\r\n          <b-button\r\n            :disabled=\"addLangBtnDisabled\"\r\n            variant=\"outline-primary\"\r\n            @click=\"addLang()\"\r\n            style=\"margin-top: 0.5rem; margin-left: 1rem\"\r\n            >Add</b-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- GENDER -->\r\n      <div class=\"mt-2 pt-2\">\r\n        Pronouns\r\n        <div v-for=\"(pronoun, index) in pronouns\" :key=\"pronoun\">\r\n          <input type=\"checkbox\" class=\"\" v-model=\"userPronouns[index]\" />\r\n          <span style=\"margin-left: 0.5rem\">{{ pronouns[index] }} </span>\r\n        </div>\r\n      </div>\r\n      <!-- MUSIC -->\r\n      <div class=\"mt-2\">\r\n        <label for=\"music-list-id\">Music</label>\r\n        <div\r\n          v-for=\"musicKey in userMusicCount\"\r\n          :key=\"musicKey\"\r\n          style=\"display: flex; flex-direction: row\"\r\n        >\r\n          <b-form-input\r\n            readonly\r\n            :placeholder=\"userMusic[musicKey - 1]\"\r\n            style=\"max-width: 50%; margin-top: 0.5rem\"\r\n            disabled\r\n          ></b-form-input>\r\n          <b-button\r\n            variant=\"outline-danger\"\r\n            @click=\"removeMusic(musicKey)\"\r\n            style=\"margin-top: 0.5rem; margin-left: 1rem\"\r\n            >Remove</b-button\r\n          >\r\n        </div>\r\n        <div style=\"display: flex; flex-direction: row\">\r\n          <b-form-input\r\n            v-model=\"userCurrentMusic\"\r\n            list=\"musiclist-id\"\r\n            placeholder=\"Add Genre\"\r\n            style=\"max-width: 50%; margin-top: 0.5rem\"\r\n          ></b-form-input>\r\n          <datalist id=\"musiclist-id\">\r\n            <option>Manual Option</option>\r\n            <option v-for=\"music in musicList\" :key=\"music.genre\">\r\n              {{ music.genre }}\r\n            </option>\r\n          </datalist>\r\n          <b-button\r\n            :disabled=\"addMusicBtnDisabled\"\r\n            variant=\"outline-primary\"\r\n            @click=\"addMusic()\"\r\n            style=\"margin-top: 0.5rem; margin-left: 1rem\"\r\n            >Add</b-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- ARTISTIC -->\r\n      <div class=\"mt-2\">\r\n        <label for=\"art-list-id\">Art-Style</label>\r\n        <div\r\n          v-for=\"artKey in userArtCount\"\r\n          :key=\"artKey\"\r\n          style=\"display: flex; flex-direction: row\"\r\n        >\r\n          <b-form-input\r\n            readonly\r\n            :placeholder=\"userArt[artKey - 1]\"\r\n            style=\"max-width: 50%; margin-top: 0.5rem\"\r\n            disabled\r\n          ></b-form-input>\r\n          <b-button\r\n            variant=\"outline-danger\"\r\n            @click=\"removeArt(artKey)\"\r\n            style=\"margin-top: 0.5rem; margin-left: 1rem\"\r\n            >Remove</b-button\r\n          >\r\n        </div>\r\n        <div style=\"display: flex; flex-direction: row\">\r\n          <b-form-input\r\n            v-model=\"userCurrentArt\"\r\n            list=\"artlist-id\"\r\n            placeholder=\"Add Genre\"\r\n            style=\"max-width: 50%; margin-top: 0.5rem\"\r\n          ></b-form-input>\r\n          <datalist id=\"artlist-id\">\r\n            <option>Manual Option</option>\r\n            <option v-for=\"art in artList\" :key=\"art.genre\">\r\n              {{ art.genre }}\r\n            </option>\r\n          </datalist>\r\n          <b-button\r\n            :disabled=\"addArtBtnDisabled\"\r\n            variant=\"outline-primary\"\r\n            @click=\"addArt()\"\r\n            style=\"margin-top: 0.5rem; margin-left: 1rem\"\r\n           